:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x560291523440 .scope module, "Half_Adder_tb" "Half_Adder_tb" 2 3;
 .timescale -9 -11;
v0x560291534d50_0 .var "A", 0 0;
v0x560291534e10_0 .var "B", 0 0;
v0x560291534ee0_0 .net "Carry", 0 0, L_0x5602915350b0;  1 drivers
v0x560291534fe0_0 .net "Sum", 0 0, L_0x560291535210;  1 drivers
E_0x560291524150 .event edge, v0x560291534b10_0, v0x560291534be0_0;
S_0x5602915235f0 .scope module, "UUT" "Half_Adder" 2 8, 3 2 0, S_0x560291523440;
 .timescale -9 -11;
    .port_info 0 /OUTPUT 1 "Sum";
    .port_info 1 /OUTPUT 1 "Carry";
    .port_info 2 /INPUT 1 "A";
    .port_info 3 /INPUT 1 "B";
L_0x5602915350b0 .functor AND 1, v0x560291534d50_0, v0x560291534e10_0, C4<1>, C4<1>;
L_0x560291535210 .functor XOR 1, v0x560291534d50_0, v0x560291534e10_0, C4<0>, C4<0>;
v0x5602914e7d20_0 .net "A", 0 0, v0x560291534d50_0;  1 drivers
v0x560291534a50_0 .net "B", 0 0, v0x560291534e10_0;  1 drivers
v0x560291534b10_0 .net "Carry", 0 0, L_0x5602915350b0;  alias, 1 drivers
v0x560291534be0_0 .net "Sum", 0 0, L_0x560291535210;  alias, 1 drivers
    .scope S_0x560291523440;
T_0 ;
    %vpi_call 2 12 "$dumpfile", "Half_Adder_tb.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x560291523440 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560291534d50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560291534e10_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x560291523440;
T_1 ;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560291534d50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560291534e10_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560291534d50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560291534e10_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560291534d50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560291534e10_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560291534d50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560291534e10_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x560291523440;
T_2 ;
    %wait E_0x560291524150;
    %vpi_call 2 41 "$display", "time =%0t \011 A =%b, B =%b, Sum =%b and Carry =%b", $time, v0x560291534d50_0, v0x560291534e10_0, v0x560291534fe0_0, v0x560291534ee0_0 {0 0 0};
    %jmp T_2;
    .thread T_2, $push;
# The file index is used to find the file name in the following table.
